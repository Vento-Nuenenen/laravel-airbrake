{
    "name": "vento/laravel-airbrake",
    "description": "Laravel service provider for Airbrake https://github.com/airbrake/phpbrake",
    "keywords": ["airbrake", "laravel"],
    "require": {
        "php": ">=7.2",
        "airbrake/phpbrake": "^0.7.3",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vento",
            "email": "github@brenneisen.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kouz\\LaravelAirbrake\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Kouz\\LaravelAirbrake\\ServiceProvider"
            ]
        }
    }
}
